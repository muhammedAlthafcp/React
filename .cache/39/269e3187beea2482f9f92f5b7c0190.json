\n  }\r\n  \r\n  .res-card-desc {\r\n    font-size: 14px;\r\n    color: #555;\r\n    padding: 0 10px 10px;\r\n  }\r\n  \r\n\r\n  /* shimmer */\r\n\r\n  /* Shimmer Card */\r\n.shimmer-card {\r\n  width: 250px;\r\n  background-color: #f4f4f9;\r\n  border-radius: 10px;\r\n  overflow: hidden;\r\n  text-align: center;\r\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n}\r\n\r\n.shimmer-img {\r\n  width: 100%;\r\n  height: 150px;\r\n  background: linear-gradient(90deg, #e0e0e0 25%, #f4f4f9 50%, #e0e0e0 75%);\r\n  background-size: 200% 100%;\r\n  animation: shimmer 1.5s infinite;\r\n}\r\n\r\n.shimmer-line {\r\n  height: 15px;\r\n  margin: 10px auto;\r\n  background: linear-gradient(90deg, #e0e0e0 25%, #f4f4f9 50%, #e0e0e0 75%);\r\n  background-size: 200% 100%;\r\n  animation: shimmer 1.5s infinite;\r\n}\r\n\r\n.shimmer-title {\r\n  width: 70%;\r\n  height: 20px;\r\n}\r\n\r\n.shimmer-desc {\r\n  width: 85%;\r\n  height: 15px;\r\n}\r\n\r\n.shimmer-rating,\r\n.shimmer-time {\r\n  width: 50%;\r\n  height: 15px;\r\n}\r\n\r\n/* Shimmer Animation */\r\n@keyframes shimmer {\r\n  0% {\r\n    background-position: -200% 0;\r\n  }\r\n  100% {\r\n    background-position: 200% 0;\r\n  }\r\n}\r\n  \r\n/* shimmer */\r\n\r\n.search-btn {\r\n  width: 100px;\r\n  height: 39px;\r\n  background-color: #fff;\r\n  border: 1px solid;\r\n  border-color: #ccc;\r\n  margin-left: 2px;\r\n  font-size: 16px;\r\n\r\n}\r\n.search-btn:hover{\r\n  background-color: red;\r\n  font-weight: bold;\r\n\r\n}"},"lineCount":174}},"error":null,"hash":"03c857c7d971b510467d8c01998fe747","cacheData":{"env":{}}}